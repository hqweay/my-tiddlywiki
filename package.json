{
  "name": "hqweay-wiki",
  "version": "0.0.0",
  "description": "hqweay's wiki",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "if [ -e wiki.html ]; then cp -rf wiki.html index.html; else tiddlywiki --build index && mv output/index.html . && cp index.html wiki.html; fi;",
    "start": "tiddlywiki --listen port=1081 anon-username=hqweay",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hqweay/wiki.git"
  },
  "author": "hqweay <hqweay@gmail.com> (https://leay.net/)",
  "license": "MIT",
  "dependencies": {
    "tiddlywiki": "^5.1.21"
  }
}